{
	"manifest_version": 2,
	"name": "__TEMPLATE__",
	"version": "1.0.0",

	"background": {
		"scripts": [ "background.js" ]
	},

	"content_scripts": [
		{
			"matches": [ "<all_urls>" ],
			"js": [ "content.js" ]
		}
	],

	"browser_action": {
		"default_title": "__POPUP__",
		"default_popup": "popup/index.html"
	}
}
